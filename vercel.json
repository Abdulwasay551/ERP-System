{
  "version": 2,
  "builds": [
    {
      "src": "setting/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "setting/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "setting.settings",
    "VERCEL": "1",
    "USE_SQLITE": "1"
  }
} 