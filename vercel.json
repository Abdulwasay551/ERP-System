{
  "version": 2,
  "builds": [
    {
      "src": "setting/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "setting/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "setting.settings",
    "VERCEL": "1",
    "DJANGO_DEBUG": "False"
  },
  "installCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
} 